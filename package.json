{
  "name": "typesafe-custom-events",
  "version": "1.0.1",
  "description": "Lets you easily set up typesafe CustomEvent channels in Typescript",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "homepage": "https://typesafe-custom-events.ryfylke.dev/",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "author": "Ryfylke React AS",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "toast"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryfylke-react-as/typesafe-custom-events.git"
  }
}
